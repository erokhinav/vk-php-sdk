{
    "name": "vk/php-sdk",
    "description": "VK SDK for PHP",
    "type": "library",
    "homepage": "https://github.com/erokhinav/vk-php-sdk",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^5.4|^7.0",
        "vkcom/vk-api-schema": "1.4.1"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0"
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/VKCOM/vk-api-schema"
        }
    ],
    "scripts": {
      "generate-sdk": [
        "@php src/VK/Generators/generate_actions.php"
      ]
    },
    "autoload": {
      "psr-4": { "VK\\": "src/VK" }
    }
}
